{
    "name": "sqlite-admin/sqlite-admin",
    "description": "A simple SQLite Admin or SQLite viewer.",
    "type": "project",
    "license": "MIT",
    "keywords": ["SQLite","Admin","viewer","SQL"],
    "version": "0.0.2",
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
      "psr-4": {
        "Tests\\": "tests/"
      }
    },
    "authors": [
        {
            "name": "Sushil Kumar",
            "email": "sushilkumbhar77@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
      "php": "^8.0",
        "symfony/console": "^7.1",
        "symfony/finder": "^7.1",
        "symfony/process": "^7.1",
        "symfony/string": "^7.1",
        "psr/log": "^3.0",
        "league/flysystem": "^3.29",
        "league/flysystem-local": "^3.29",
        "league/mime-type-detection": "^1.16"
    },
    "require-dev": {
      "phpstan/phpstan": "^2.0",
        "pestphp/pest": "^3.5",
        "nunomaduro/collision": "^8.4"
    },
    "config": {
      "optimize-autoloader": true,
      "allow-plugins": {
        "pestphp/pest-plugin": true
      }
    }
}
